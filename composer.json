    "description": "Xloit Collection Library",
    "keywords": [
        "xloit",
        "collection",
        "library"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Yudha Setiawan",
            "email": "me@yudhasetiawan.com",
